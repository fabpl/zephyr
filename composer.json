{
  "name": "fabl/zephyr",
  "description": "Minimal Laravel authentication and profile scaffolding with Blade and Tailwind.",
  "keywords": ["laravel", "auth"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/fabpl/zephyr/issues",
    "source": "https://github.com/fabpl/zephyr"
  },
  "authors": [
    {
      "name": "Fabrice Planchette",
      "email": "planchettefabrice@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.2.0",
    "illuminate/console": "^10.0",
    "illuminate/filesystem": "^10.0",
    "illuminate/support": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Fabpl\\Zephyr\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "laravel": {
      "providers": [
        "Fabpl\\Zephyr\\ZephyrServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.10",
    "orchestra/testbench": "^8.0",
    "phpstan/phpstan": "^1.10"
  }
}
